{
  "name": "docs-web",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "node scripts/dev-single.mjs",
    "build": "next build",
    "start": "next start",
    "runner": "node scripts/runner.mjs",
    "serve": "node scripts/server.mjs",
    "test": "tsx --test tests/upload.integration.test.ts && tsx --test tests/text-formats.test.ts && tsx --test tests/latex.integration.test.ts && tsx --test tests/auth-guest.test.ts",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "build:highlight-core": "npm --workspace packages/highlight-core run build"
  },
  "dependencies": {
    "busboy": "1.6.0",
    "chokidar": "4.0.3",
    "next-auth": "5.0.0-beta.29",
    "next": "15.5.4",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@types/node": "24.6.0",
    "@types/react": "19.1.16",
    "eslint": "9.36.0",
    "eslint-config-next": "15.5.4",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "tsx": "4.20.6"
  }
}
